.wf-list-container{display:flex;justify-content:space-between;align-items:flex-start;padding:0 20rpx}.wf-list{width:calc(50% - 10rpx);display:flex;flex-direction:column}.wf-item-child{background:#fff;margin-bottom:20rpx;border-radius:16rpx;overflow:hidden}.wf-item-child .examin-btn{top:20rpx;left:15rpx;width:89rpx;height:37rpx;z-index:1}.wf-item-child .play-video-info{top:0rpx;width:100%;height:calc(100% - 128rpx);z-index:9}.wf-item-child .play-video-info .play-video{width:66rpx;height:66rpx;background:rgba(2,2,2,.5)}.wf-item-child .play-video-info .play-video .iconfont{font-size:28rpx}.cover{width:100%;height:100rpx}.wf-item{padding:20rpx}.wf-item .avatar{width:40rpx;height:40rpx;border-radius:40rpx;margin-right:6rpx}.wf-item .coach{max-width:100rpx}.wf-item .iconfont{font-size:24rpx}