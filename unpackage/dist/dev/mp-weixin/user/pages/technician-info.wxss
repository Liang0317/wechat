.technician-info .cover-img {
  width: 750rpx;
  height: 417rpx;
}
.technician-info .like-label {
  width: 42rpx;
  height: 42rpx;
  background: #EFEFEF;
}
.technician-info .like-label .iconfont {
  font-size: 24rpx;
  margin-top: 5rpx;
}
.technician-info .service-label {
  min-width: 88rpx;
  height: 32rpx;
  padding: 0 10rpx;
  color: #EBDDB1;
  background: linear-gradient(270deg, #4C545A 0%, #282B34 100%);
  border-radius: 4rpx;
}
.technician-info .time-label .near-text {
  width: 100rpx;
  height: 33rpx;
  background: #F5F5F5;
  border-radius: 3rpx 0 0 3rpx;
}
.technician-info .time-label .near-time {
  width: 66rpx;
  height: 33rpx;
  border-radius: 0 3rpx 3rpx 0;
}
.technician-info .time-label .near-time .bg {
  opacity: 0.1;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  border-radius: 0 3rpx 3rpx 0;
}
.technician-info .icondianpu {
  margin-right: 6rpx;
}
.technician-info .iconpingfen1 {
  font-size: 24rpx;
  background-image: -webkit-linear-gradient(270deg, #FAD961 0%, #F76B1C 100%);
}
.technician-info .star-text {
  height: 26rpx;
  color: #FF9519;
  margin-left: 6rpx;
}
.technician-info .line {
  width: 1rpx;
  height: 11rpx;
  background: #979797;
  margin: 0 18rpx;
}
.technician-info .introduce-info {
  width: 690rpx;
  background: #F9F9F9;
}
.technician-info .zhengshu-img {
  width: 37rpx;
  height: 40rpx;
}
.technician-info .picture-list {
  white-space: nowrap;
  width: 690rpx;
}
.technician-info .picture-list .picture-item {
  width: 122rpx;
  margin-left: 20rpx;
  display: inline-block;
}
.technician-info .picture-list .picture-item .cover {
  width: 122rpx;
  height: 122rpx;
}
.technician-info .picture-list .picture-item:nth-child(1) {
  margin-left: 0;
}
.technician-info .comment-item .avatar {
  width: 52rpx;
  height: 52rpx;
  background: #f4f6f8;
}
.technician-info .comment-item .iconpingfen1 {
  font-size: 28rpx;
  margin-right: 5rpx;
  font-size: 28rpx;
}

