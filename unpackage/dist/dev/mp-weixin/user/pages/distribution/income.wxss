.user-distribution-income .bg {
  width: 100%;
  height: 388rpx;
}
.user-distribution-income .pay-btn {
  width: 100rpx;
  height: 40rpx;
  border-radius: 5rpx;
  border: 1rpx solid #fff;
}
.user-distribution-income .mine-menu-list {
  margin: 20rpx 25rpx 0 25rpx;
}
.user-distribution-income .mine-menu-list .close-info {
  width: 80rpx;
  height: 80rpx;
  top: 0;
  right: 0;
  z-index: 1;
}
.user-distribution-income .mine-menu-list .cash-btn {
  width: 580rpx;
  height: 70rpx;
}
.user-distribution-income .mine-menu-list .avatar {
  width: 105rpx;
  height: 105rpx;
}
.user-distribution-income .mine-menu-list .share-btn {
  width: 154rpx;
  height: 56rpx;
  background: #FF6124;
}
.user-distribution-income .mine-menu-list .item-child {
  width: 33.33%;
  margin: 10rpx 0;
}
.user-distribution-income .mine-menu-list .item-child .iconfont {
  font-size: 86rpx;
}
.user-distribution-income .mine-menu-list .item-child .item-img {
  width: 88rpx;
  height: 88rpx;
}
.user-distribution-income .mine-menu-list .item-child .item-img .iconfont {
  font-size: 44rpx;
}
.user-distribution-income .mine-menu-list .item-child .item-img .item-img {
  top: 0;
  left: 0;
  opacity: 0.1;
}
.user-distribution-income .mine-menu-list .item-child:nth-child(1).b-1px-b::after {
  left: 33rpx;
}
.user-distribution-income .mine-menu-list .item-child:nth-child(2).b-1px-b::after {
  right: 33rpx;
}
.user-distribution-income .mine-menu-list .item-child.b-1px-r::after {
  top: 38rpx;
  bottom: 38rpx;
}

