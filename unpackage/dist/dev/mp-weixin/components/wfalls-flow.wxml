<view class="wf-list-container"><block wx:for="{{viewList}}" wx:for-item="list" wx:for-index="listIndex" wx:key="listIndex"><view class="wf-list" id="wf-list"><block wx:for="{{list.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="wf-item-child rel" data-id="{{item.id}}" data-event-opts="{{[['tap',[['goDetail',[listIndex,index]]]]]}}" bindtap="__e"><block wx:if="{{path==2&&item.status!=2}}"><view class="examin-btn flex-center f-icontext c-base radius abs" style="{{'background:'+(item.status==1?'#FC8218':'#FF6262')+';'}}">{{''+(item.status==1?'审核中':'已驳回')+''}}</view></block><image class="cover" mode="widthFix" id="{{'id'+item.id}}" src="{{item.cover}}" data-event-opts="{{[['load',[['handleViewRender',['$event']]]],['error',[['handleViewRender',['$event']]]]]}}" bindload="__e" binderror="__e"></image><block wx:if="{{item.type==2}}"><view class="play-video-info flex-center c-base abs"><view class="play-video flex-center c-base radius"><view class="iconfont icon-play-video _i"></view></view></view></block><view class="wf-item"><view class="f-desc c-black text-bold">{{item.title}}</view><view class="flex-between mt-sm"><view class="flex-center"><image class="avatar" mode="aspectFill" src="{{item.work_img}}"></image><view class="coach f-caption c-desc ellipsis">{{item.coach_name}}</view></view><view class="flex-y-baseline f-caption c-desc"><view class="iconfont iconjuli _i" style="{{'color:'+(primaryColor)+';'}}"></view>{{''+item.distance+''}}</view></view></view></view></block></view></block></view>