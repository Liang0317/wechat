.pages-map .map-info .iconjuli {
  font-size: 30rpx;
}
.pages-map .map-info .icon-down {
  color: #868686;
}
.pages-map .map-info .map-list-item .list-btn {
  width: 92rpx;
  height: 46rpx;
  -webkit-transform: rotateZ(360deg);
          transform: rotateZ(360deg);
  border-radius: 4rpx 0 0 4rpx;
  border: 1rpx solid #DADFE3;
}
.pages-map .map-info .map-list-item .list-btn .iconfont {
  font-size: 22rpx;
  margin-right: 4rpx;
}
.pages-map .map-info .map-list-item .list-btn:nth-child(2) {
  border-radius: 0 4rpx 4rpx 0;
}
.pages-map .map-info .map-list-item .list-btn.abs {
  opacity: 0.3;
  border: none;
}
.pages-map .tab-info .tab-list-item {
  width: 90%;
}
.pages-map .tab-info .tab-select-item {
  height: 44rpx;
  box-shadow: -5px 0 5px -5px rgba(0, 0, 0, 0.8);
}
.pages-map .map-box {
  width: 100%;
}
.pages-map .search-popup {
  width: calc(100% - 164rpx);
  height: 100vh;
}
.pages-map .search-popup .list-item {
  width: 160rpx;
  height: 70rpx;
  background: #F5F5F5;
}
.pages-map .search-popup .list-item .icon-guanbi-fill.abs {
  top: -15rpx;
  right: 0;
  font-size: 50rpx;
}
.pages-map .search-popup .list-item:nth-child(3n) {
  margin-right: 0;
}
.pages-map .search-popup .search-btn {
  width: 230rpx;
  height: 80rpx;
  border: 1rpx solid #C7C7C7;
  -webkit-transform: rotateZ(360deg);
          transform: rotateZ(360deg);
}
.pages-map .technician-info-popup {
  width: 680rpx;
  overflow: hidden;
}
.pages-map .technician-info-popup .work-img {
  width: 680rpx;
  height: 680rpx;
}
.pages-map .technician-info-popup .service-label {
  min-width: 88rpx;
  height: 32rpx;
  padding: 0 10rpx;
  color: #EBDDB1;
  background: linear-gradient(270deg, #4C545A 0%, #282B34 100%);
  border-radius: 4rpx;
}
.pages-map .technician-info-popup .time-label .near-text {
  width: 100rpx;
  height: 33rpx;
  background: #F5F5F5;
  border-radius: 3rpx 0 0 3rpx;
}
.pages-map .technician-info-popup .time-label .near-time {
  width: 66rpx;
  height: 33rpx;
  border-radius: 0 3rpx 3rpx 0;
}
.pages-map .technician-info-popup .time-label .near-time .bg {
  opacity: 0.1;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  border-radius: 0 3rpx 3rpx 0;
}
.pages-map .technician-info-popup .icondianpu {
  margin-right: 6rpx;
}
.pages-map .technician-info-popup .iconpingfen1 {
  font-size: 24rpx;
  background-image: -webkit-linear-gradient(270deg, #FAD961 0%, #F76B1C 100%);
}
.pages-map .technician-info-popup .star-text {
  height: 26rpx;
  color: #FF9519;
  margin-left: 6rpx;
}
.pages-map .technician-info-popup .line {
  width: 1rpx;
  height: 11rpx;
  background: #979797;
  margin: 0 18rpx;
}
.pages-map .technician-info-popup .introduce-info {
  width: 630rpx;
  max-height: 250rpx;
  overflow-y: auto;
  background: #F9F9F9;
}
.pages-map .icon-close {
  font-size: 60rpx;
}

