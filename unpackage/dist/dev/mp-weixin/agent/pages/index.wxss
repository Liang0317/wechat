.agent-index .mine-count-list {
  width: 710rpx;
  height: 295rpx;
  margin: 20rpx 20rpx 0 20rpx;
}
.agent-index .mine-count-list .text {
  color: rgba(255, 255, 255, 0.6);
}
.agent-index .mine-count-list .cash-out-btn {
  width: 180rpx;
  height: 70rpx;
}
.agent-index .mine-count-list .count-data-list .list-item {
  width: 50%;
}
.agent-index .dot-unread-number {
  top: 0;
  right: 0;
  width: 24rpx;
  height: 24rpx;
  line-height: 24rpx;
  text-align: center;
  color: #fff;
  font-size: 18rpx;
  border-radius: 24rpx;
  background-color: #F1381F;
}
.agent-index .mine-menu-list {
  margin: 20rpx 20rpx 0 20rpx;
}
.agent-index .mine-menu-list .menu-title {
  height: 90rpx;
  color: #434343;
}
.agent-index .mine-menu-list .menu-title .iconfont {
  font-size: 24rpx;
}
.agent-index .mine-menu-list .icon-info {
  width: 80rpx;
  height: 80rpx;
  background: #FFFFFF;
  box-shadow: 0 3rpx 31rpx -2rpx rgba(217, 224, 219, 0.5);
  border-radius: 29rpx;
  margin-right: 16rpx;
}
.agent-index .mine-menu-list .icon-info .iconfont {
  font-size: 48rpx;
}
.agent-index .mine-menu-list .share-btn {
  width: 130rpx;
  height: 52rpx;
  border-radius: 8rpx;
}
.agent-index .mine-menu-list .notice-info .title {
  width: 100rpx;
  height: 34rpx;
  border-radius: 4rpx;
}
.agent-index .mine-menu-list .notice-info .iconfont {
  font-size: 20rpx;
}
.agent-index .mine-menu-list .item-child {
  width: 25%;
  margin: 10rpx 0;
}
.agent-index .mine-menu-list .item-child .iconfont {
  font-size: 52rpx;
}
.agent-index .mine-menu-list .item-child .item-img {
  width: 60rpx;
  height: 60rpx;
}
.agent-index .mine-menu-list .item-child .item-img .iconfont {
  font-size: 36rpx;
}
.agent-index .mine-menu-list .item-child .item-img .item-img {
  top: 0;
  left: 0;
  opacity: 0.1;
}
.agent-index .mine-menu-list .item-child .icon-img {
  height: 32rpx;
}
.agent-index .mine-menu-list.share {
  padding: 30rpx 30rpx 30rpx 20rpx;
}

